{
  "name": "akos-studio",
  "version": "0.1.0",
  "private": true,
  "main": "dist/studio/electron/main.js",
  "scripts": {
    "build": "npm run compile && electron-builder --dir",
    "compile": "ng build --prod --base-href ./ && tsc -p electron && gulp",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "serve:electron": "wait-on http-get://localhost:4200/ && tsc -p electron && electron . -s",
    "serve:web": "ng serve",
    "start": "npm-run-all -p serve:web serve:electron",
    "test": "ng test"
  },
  "build": {
    "files": [
      "dist/studio",
      "!**/*.js.map"
    ],
    "win": {
      "target": "portable"
    }
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.2",
    "@angular/animations": "^8.2.2",
    "@angular/cdk": "^8.1.3",
    "@angular/cli": "^8.2.2",
    "@angular/common": "^8.2.2",
    "@angular/compiler": "^8.2.2",
    "@angular/compiler-cli": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@angular/forms": "^8.2.2",
    "@angular/language-service": "^8.2.2",
    "@angular/material": "^8.1.3",
    "@angular/platform-browser": "^8.2.2",
    "@angular/platform-browser-dynamic": "^8.2.2",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "core-js": "^2.6.9",
    "electron": "^6.0.2",
    "electron-builder": "^21.2.0",
    "electron-reload": "^1.5.0",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "material-design-icons-iconfont": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "rxjs": "~6.5.2",
    "ts-node": "~7.0.0",
    "tslib": "^1.10.0",
    "tslint": "~5.11.0",
    "typeface-roboto": "^0.0.75",
    "typescript": "~3.4.5",
    "wait-on": "^3.3.0",
    "zone.js": "~0.9.1"
  }
}
