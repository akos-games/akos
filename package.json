{
  "name": "akos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build:editor",
    "build:editor": "ng build editor --prod --base-href ./ && tsc -p projects/editor/src/electron && electron-builder --dir -c projects/editor/electron-builder.yml",
    "e2e": "ng e2e",
    "electron:editor": "wait-on http-get://localhost:4200/ && tsc -p projects/editor/src/electron && electron --inspect=5858 dist/editor/index.js -s",
    "lint": "ng lint",
    "ng": "ng",
    "serve:editor": "ng serve editor",
    "start": "npm run start:editor",
    "start:editor": "npm-run-all -p serve:editor electron:editor",
    "test": "ng test"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.15",
    "@angular-devkit/build-ng-packagr": "~0.803.15",
    "@angular/animations": "~8.2.12",
    "@angular/cdk": "~8.2.3",
    "@angular/cli": "~8.3.15",
    "@angular/common": "~8.2.12",
    "@angular/compiler": "~8.2.12",
    "@angular/compiler-cli": "~8.2.12",
    "@angular/core": "~8.2.12",
    "@angular/forms": "~8.2.12",
    "@angular/language-service": "~8.2.12",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.12",
    "@angular/platform-browser-dynamic": "~8.2.12",
    "@angular/router": "~8.2.12",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "electron": "^7.0.0",
    "electron-builder": "^22.1.0",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "material-design-icons-iconfont": "^5.0.1",
    "ng-packagr": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "rxjs": "~6.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslib": "^1.10.0",
    "tslint": "~5.15.0",
    "typeface-roboto": "0.0.75",
